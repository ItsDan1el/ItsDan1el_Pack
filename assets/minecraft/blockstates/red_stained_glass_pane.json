{
    "multipart": [
        {   "apply": { "model": "block/iron_bars_post_ends2" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "block/iron_bars_post2" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "block/iron_bars_cap2" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "block/iron_bars_cap2", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "block/iron_bars_cap_alt2" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "block/iron_bars_cap_alt2", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "block/iron_bars_side2" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/iron_bars_side2", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/iron_bars_side_alt2" }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/iron_bars_side_alt2", "y": 90 }
        }
    ]
}
